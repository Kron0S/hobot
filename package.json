{
  "name": "matching-bot",
  "version": "0.0.1",
  "description": "Telegram bot for matching different requests and responses",
  "main": "index.ts",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p . --lib es2017 --outDir dist && npm run dist-locales",
    "dist-locales": "cp -a ./src/locales/ ./dist/locales",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "tmp": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/tmp.ts"
  },
  "keywords": [
    "matching",
    "telegram"
  ],
  "author": "Andrey Selin",
  "license": "SEE LICENSE IN LICENCE",
  "dependencies": {
    "@types/node": "^11.11.3",
    "bluebird": "^3.5.5",
    "nodemon": "^1.18.10",
    "telegraf": "^3.28.0",
    "telegraf-i18n": "^6.5.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  }
}
